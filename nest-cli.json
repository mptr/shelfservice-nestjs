{
	"$schema": "https://json.schemastore.org/nest-cli",
	"collection": "@nestjs/schematics",
	"sourceRoot": "src",
	"compilerOptions": {
		"assets": ["**/*.yaml", "**/*.yml"]
	},
	"projects": {
		"api": {
			"type": "application",
			"entryFile": "main"
		},
		"schedule": {
			"type": "microservice",
			"entryFile": "schedule"
		}
	}
}
